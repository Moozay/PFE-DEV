<!DOCTYPE html>
<html>
  <head>
    <title>Classes</title>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="/css/listedeclasse.css" />
  </head>
  <body>
    <div
      class="container-flex"
    >
      <div id="premiere" onclick="callpage(this.id)">
        PREMIERE
      </div>
      <div onclick="callpage(this.id)" id="seconde">
        SECONDE
      </div>
      <div onclick="callpage(this.id)" id="terminale">
        TERMINALE
      </div>
      <div onclick="callpage(this.id)" id="dashboard">
        DASHBOARD
      </div>
    </div>
    <script>
      var getCookies = function(){
  var pairs = document.cookie.split(";");
  var cookies = {};
  for (var i=0; i<pairs.length; i++){
    var pair = pairs[i].split("=");
    cookies[(pair[0]+'').trim()] = unescape(pair.slice(1).join('='));
  }
  return cookies;
}

var myCookies = getCookies();


    function callpage(id) {
      switch (id) {
        case 'premiere':
          location.assign('/bulletins?id=premiere')
          break;
        case 'seconde':
          location.assign('/bulletins?id=seconde')
          break;
        case 'terminale':
          location.assign('/bulletins?id=terminale')
          break;
        case 'dashboard':
          if (myCookies.role == 'directeur') {
            location.assign('/accueildirecteur')
          }else if (myCookies.role == 'surveillant') {
            location.assign('/accueilsurveillant')
          }
          break;
    
        default:
          break;
      }
    }    
    </script>
