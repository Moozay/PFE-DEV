<% for(var i = 0; i < users.length; i++) { %>
    <tr>
        <td><%= users[i].identifiant %> </td>
        <td><%= users[i].Firstname %> </td>
        <td><%= users[i].Lastname %> </td>
        <td><%= users[i].Email %> </td>
        <td><%= users[i].role %> </td>
        <td>
            <a href="/update-user?id=<%= users[i]._id%>" class="btn border-shadow update">
                <span class="text-gradient"><i>UPDATE</i></span>
            </a>
            <a class="btn border-shadow delete" data-id=<%= users[i]._id%> >
                <span class="text-gradient"><i>DELETE</i></span>
            </a>
        </td>
    </tr>
    <% } %>