<% for(var i = 0; i < eleves.length; i++) { %>
    <tr>
        <td><%= eleves[i].codeapogee %> </td>
        <td><%= eleves[i].nom %> </td>
        <td><%= eleves[i].prenom %> </td>
        <td><%= eleves[i].age %> </td>
        <td><%= classe%> </td>
        <td>
            <a onclick="window.open('/bulletineleve?id=<%= eleves[i]._id%>&classe=<%=classe%>', '_blank').focus(); " class="btn border-shadow update">
                <span class="text-gradient"><i>Voir</i></span>
            </a>
        </td>
    </tr>
    <% } %>